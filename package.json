{
    "name": "email",
    "private": true,
    "workspaces": [
        "packages/*"
    ],
    "scripts": {
        "remove:all": "lerna run remove:all --stream --parallel",
        "clean": "lerna run clean --stream --parallel",
        "build:lib": "lerna run build:lib --stream --parallel",
        "build:client": "yarn workspace @email/client build-client",
        "start": "yarn workspace @email/client start",
        "typecheck": "tsc --noEmit",
        "prettify": "npx prettier \"./**/*.{js,ts,tsx}\" --write"
    },
    "devDependencies": {
        "@types/node": "^13.13.5",
        "@types/react": "^16.9.34",
        "@types/react-dom": "^16.9.7",
        "@types/underscore": "^1.10.0",
        "lerna": "^3.20.2",
        "rimraf": "^3.0.2",
        "ts-node": "^8.10.1",
        "tsc-watch": "^4.2.3",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "typescript": "^3.8.3",
        "underscore": "^1.10.2"
    }
}