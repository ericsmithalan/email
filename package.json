{
    "name": "email",
    "private": true,
    "workspaces": [
        "packages/*"
    ],
    "scripts": {
        "clean": "lerna run clean --stream --parallel",
        "build": "lerna run build --stream --parallel",
        "lint": "eslint . --ext .js,.ts,.tsx -f codeframe",
        "pretest": "yarn typecheck && yarn lint",
        "test": "yarn test:unit",
        "start": "yarn start:client",
        "start:client": "yarn workspace @email/client start",
        "typecheck": "tsc --noEmit",
        "prettify": "npx prettier \"./**/*.{js,ts,tsx}\" --write"
    },
    "devDependencies": {
        "@ts-tools/node": "^1.1.3",
        "@ts-tools/webpack-loader": "^1.1.4",
        "@types/express": "^4.17.6",
        "@types/node": "^13.13.5",
        "@types/react": "^16.9.34",
        "@types/react-dom": "^16.9.7",
        "css-loader": "^3.5.3",
        "file-loader": "^6.0.0",
        "html-webpack-plugin": "^4.3.0",
        "lerna": "^3.20.2",
        "mini-css-extract-plugin": "^0.9.0",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "rimraf": "^3.0.2",
        "tsconfig-paths": "^3.9.0",
        "tsconfig-paths-webpack-plugin": "^3.2.0",
        "typescript": "^3.8.3",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.11",
        "webpack-dev-server": "^3.10.3",
        "@types/underscore": "^1.10.0",
        "csstype": "^2.6.10",
        "typescript-collections": "^1.3.3",
        "underscore": "^1.10.2",
        "tsc-watch": "^4.2.3"
    },
    "dependencies": {}
}
