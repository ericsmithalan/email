{
    "name": "@email/css",
    "version": "1.0.0",
    "private": true,
    "main": "cjs/index.js",
    "module": "esm/index.js",
    "types": "esm/index.d.ts",
    "scripts": {
        "clean": "rimraf ./cjs ./esm",
        "prebuild": "yarn clean",
        "build-lib": "yarn build:cjs && yarn build:esm",
        "build:cjs": "ts-watch -p tsconfig.build.json --module commonjs --outDir cjs",
        "build:esm": "ts-watch -p tsconfig.build.json --module esnext --outDir esm",
        "test": "mocha \"test/**/*.spec.ts?(x)\"",
        "prepack": "yarn build"
    },
    "files": [
        "cjs",
        "umd",
        "src"
    ],
    "publishConfig": {
        "access": "public"
    },
    "peerDependencies": {
        "react": ">=16",
        "react-dom": ">=16",
        "tsc-watch": ">=4",
        "typescript": ">=3",
        "underscore": ">=1"
    },
    "dependencies": {
        "@types/deepmerge": "^2.2.0",
        "csstype": "^2.6.10",
        "deepmerge": "^4.2.2"
    },
    "sideEffects": false,
    "devDependencies": {
        "ts-watch": "^1.0.8"
    }
}
