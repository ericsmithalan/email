{
    "name": "@email/element",
    "version": "1.0.0",
    "private": true,
    "main": "cjs/index.js",
    "module": "esm/index.js",
    "types": "esm/index.d.ts",
    "scripts": {
        "clean": "rimraf ./cjs ./esm",
        "prebuild": "yarn clean",
        "build": "yarn build:cjs && yarn build:esm",
        "build:cjs": "tsc-watch  -p tsconfig.build.json --module commonjs --outDir cjs",
        "build:esm": "tsc-watch  -p tsconfig.build.json --module esnext --outDir esm",
        "prepack": "yarn build"
    },
    "files": [
        "cjs",
        "umd",
        "src"
    ],
    "peerDependencies": {
        "react": ">=16"
    },
    "dependencies": {
        "@email/css": "^1.0.0",
        "@types/immutable": "^3.8.7",
        "immutable": "^4.0.0-rc.12",
        "tsc-watch": "^4.2.3"
    },
    "publishConfig": {
        "access": "public"
    },
    "devDependencies": {
        "fork-ts-checker-webpack-plugin": "^4.1.3",
        "tsc-watch": "^4.2.3"
    }
}
