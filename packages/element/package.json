{
    "name": "@email/element",
    "version": "1.0.0",
    "private": true,
    "main": "cjs/index.js",
    "module": "esm/index.js",
    "types": "esm/index.d.ts",
    "scripts": {
        "clean": "rimraf ./cjs ./esm",
        "prebuild": "yarn clean",
        "build-lib": "yarn build:cjs && yarn build:esm",
        "build:cjs": "ts-watch -p tsconfig.build.json --module commonjs --outDir cjs",
        "build:esm": "ts-watch -p tsconfig.build.json --module esnext --outDir esm",
        "test": "mocha \"test/**/*.spec.ts?(x)\"",
        "prepack": "yarn build"
    },
    "files": [
        "cjs",
        "umd",
        "src"
    ],
    "peerDependencies": {
        "react": ">=16",
        "react-dom": ">=16",
        "tsc-watch": ">=4",
        "typescript": ">=3"
    },
    "dependencies": {
        "@email/css": "1.0.0"
    },
    "publishConfig": {
        "access": "public"
    },
    "devDependencies": {
        "ts-watch": "^1.0.8"
    }
}
