{
    "name": "@email/component",
    "version": "1.0.0",
    "private": true,
    "main": "dist/index.js",
    "module": "dist/index.module.js",
    "types": "dist/index.d.ts",
    "source": "src/index.tsx",
    "unpkg": "dist/index.umd.js",
    "scripts": {
        "remove:all": "yarn clean:node & yarn clean",
        "clean:node": "rimraf ./node_modules",
        "clean": "rimraf ./dist",
        "build:lib": "microbundle --jsx React.createElement --watch",
        "dev": "microbundle watch"
    },
    "files": [
        "dist"
    ],
    "publishConfig": {
        "access": "public"
    },
    "peerDependencies": {
        "react": ">=16",
        "react-dom": ">=16",
        "tsc-watch": ">=4",
        "typescript": ">=3"
    },
    "dependencies": {
        "@email/element": "1.0.0",
        "@email/css": "1.0.0"
    },
    "sideEffects": false,
    "devDependencies": {
        "ts-watch": "^1.0.8",
        "microbundle": "^0.12.0"
    }
}