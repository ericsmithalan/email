{
    "name": "@email/css",
    "version": "1.0.0",
    "private": true,
    "main": "cjs/index.js",
    "module": "esm/index.js",
    "types": "esm/index.d.ts",
    "scripts": {
        "clean": "rimraf ./cjs ./esm",
        "prebuild": "yarn clean",
        "build": "yarn build:cjs && yarn build:esm",
        "build:cjs": "tsc-watch -p tsconfig.build.json --module commonjs --outDir cjs",
        "build:esm": "tsc-watch -p tsconfig.build.json --module esnext --outDir esm",
        "test": "mocha \"test/**/*.spec.ts?(x)\"",
        "prepack": "yarn build"
    },
    "files": [
        "cjs",
        "umd",
        "src"
    ],
    "peerDependencies": {
        "react": ">=16"
    },
    "dependencies": {
        "@email/theme": "^1.0.0",
        "@types/deepmerge": "^2.2.0",
        "@types/express": "^4.17.6",
        "@types/immutable": "^3.8.7",
        "deepmerge": "^4.2.2",
        "immutable": "^4.0.0-rc.12"
    },
    "publishConfig": {
        "access": "public"
    }
}