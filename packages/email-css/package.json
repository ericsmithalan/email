{
    "name": "@email/css",
    "version": "1.0.0",
    "private": true,
    "main": "cjs/index.js",
    "module": "esm/index.js",
    "types": "esm/index.d.ts",
    "scripts": {
        "clean": "rimraf ./cjs ./esm",
        "prebuild": "yarn clean",
        "lint": "eslint . --ext .js,.ts,.tsx -f codeframe",
        "build": "yarn build:cjs && yarn build:esm",
        "build:cjs": "tsc-watch -p tsconfig.build.json --module commonjs --outDir cjs",
        "build:esm": "tsc-watch -p tsconfig.build.json --module esnext --outDir esm",
        "test": "mocha \"test/**/*.spec.ts?(x)\"",
        "prepack": "yarn build"
    },
    "files": [
        "cjs",
        "umd",
        "src"
    ],
    "peerDependencies": {
        "react": ">=16",
        "react-dom": ">=16",
        "tsc-watch": ">=4",
        "typescript": ">=3",
        "underscore": ">=1"
    },
    "dependencies": {
        "@email/theme": "^1.0.0",
        "@types/deepmerge": "^2.2.0",
        "deepmerge": "^4.2.2"
    },
    "devDependencies": {
        "ts-node": "^8.10.1"
    }
}
